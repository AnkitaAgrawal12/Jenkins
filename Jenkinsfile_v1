pipeline {
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git 'https://github.com/AnkitaAgrawal12/Jenkins.git'
            }
        }
        stage('Build Code') {
            steps {
                script {
                checkout scm
                sh 'chmod u+x prog.py'
                sh './prog.py'
            }
        }
    }

        stage('Test Code') {
            steps {
                script {
                checkout scm
                sh 'chmod u+x test.py'
                sh './test.py'
            }
        }
    } 
}
